<!DOCTYPE html>
<html lang="en">
<%- include('partials/head') %>

  <body>
    <form action="/attendance" method="post">
    <input type="text" name="code" placeholder="code">
    <input type="submit" value="regist" class="btn btn-lg btn-dark">
  </form>
    <div class="container">
      <table class="table">
        <thead>
          <tr>
            <th scope="col">#</th>
            <th scope="col">First Name</th>
            <th scope="col">Last Name</th>
            <th scope="col">code</th>
            <th scope="col">Time</th>
          </tr>
        </thead>
        <tbody>
            <% objstudent.forEach((currentItem,j) => { %>
            <tr>
              <th scope="row"> </th>
              <td>
                <%= currentItem.fname %>
              </td>
              <td>
                <%= currentItem.lname %>
              </td>
              <td>
                <%= currentItem.code %>
              </td>
              <td>
                <%= arrAttDate[j] %>
              </td>
            </tr>  
            <% }); %>
          <% arrSimilarStudents.forEach((item,i)=> { %>
            <tr>
              <th scope="row"> </th>
              <td>
                <%= item.fname %>
              </td>
              <td>
                <%= item.lname %>
              </td>
              <td>
                <%= item.code %>
              </td>
              <td>
                <%= arrAttDate[i] %>
              </td>
            </tr>
            <% }); %>
        </tbody>
      </table>
    </div>
    <div class="container">
      <a href="index" class="btn btn-lg btn-dark">Back</a>
    </div>
  </body>

</html>