<!DOCTYPE html>
<html lang="en">
  <%- include('partials/head') %>
<body>
  <div class="container">
    <table class="table">
      <thead>
        <tr>
          <th scope="col">First Name</th>
          <th scope="col">Last Name</th>
          <th scope="col">code</th>
          <th scope="col">level</th>
          <th scope="col">delete</th>
        </tr>
      </thead>
      <tbody>
        <% arrstudent.forEach((item)=> { %>
          <tr>
            <td>
              <%= item.fname %>
            </td>
            <td>
              <%= item.lname %>
            </td>
            <td>
              <%= item.code %>
            </td>
            <td>
              <%= item.level %>
            </td>
            <td>
              <a href="/delete/<%= item._id %>" class="btn btn-danger" >delete</a>
            </td>
          </tr>
          <% }); %>
      </tbody>
    </table>
  </div>
  <div class="container">
    <a href="main-delete" class="btn btn-lg btn-dark">Back</a>
  </div>
</body>
</html>